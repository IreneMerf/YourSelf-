    <Button
        android:id="@+id/button"
        android:layout_width="245dp"
        android:layout_height="64dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="#FF5050"
        android:onClick="btn_nth"
        android:text="Что нужно иметь каждому"
        app:layout_constraintEnd_toEndOf="@+id/imageView24"
        app:layout_constraintHorizontal_bias="0.809"
        app:layout_constraintStart_toStartOf="@+id/imageView24"
        app:layout_constraintTop_toTopOf="@+id/imageView24" />

    <Button
        android:id="@+id/button2"
        android:layout_width="246dp"
        android:layout_height="58dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="#FF5050"
        android:text="ЧС"
        android:onClick="btnEmergency"
        app:layout_constraintEnd_toEndOf="@+id/imageView23"
        app:layout_constraintHorizontal_bias="0.785"
        app:layout_constraintStart_toStartOf="@+id/imageView23"
        app:layout_constraintTop_toTopOf="@+id/imageView23" />

    <Button
        android:id="@+id/button3"
        android:layout_width="244dp"
        android:layout_height="57dp"
        android:layout_marginTop="24dp"
        android:backgroundTint="#FF5050"
        android:text="Первая помощь"
        android:onClick="btn_firstAid"
        app:iconTint="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/imageView22"
        app:layout_constraintHorizontal_bias="0.773"
        app:layout_constraintStart_toStartOf="@+id/imageView22"
        app:layout_constraintTop_toTopOf="@+id/imageView22" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="86dp"
